<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Katalyst</title>

    <!--about meta -->
    <meta name="theme-color" content="#6610f2">
    <link rel="icon" href="./img/instructors/call.png">

    <!-- Styles -->
    <link rel="stylesheet" href="./css/bootstrap.css">
    <link rel="stylesheet" href="./css/about.css">
    <link rel="stylesheet" href="./css/all.min.css">
    <link rel="stylesheet" href="./css/newProfile.css">
    <link rel="stylesheet" href="./css/newIndex.css">
    <link rel="stylesheet" href="./css/contact.css">

    <!-- Pre load script -->

</head>
<body>
    
    <div class="bodyWrap" id="bodyWrap">

        <header>
            <nav>
               <div class="menu-icon">
                  <i class="fa fa-bars fa-2x menuOpenIcon"></i>
               </div>
               <div class="logo">
                  KATALYST
               </div>
               <div class="menu">
                  <ul>
                     <li><a href="/">Home</a></li>
                     <li><a href="/about-us">About</a></li>
                     <li><a href="/news">News</a></li>
                     <li><a href="/contact">Contact</a></li>
                     <li><a href="/dash">Dashboard</a></li>
                  </ul>
               </div>
            </nav>
         </header>
         <div class="mainHeadNotification">
            <i class="fas fa-bell" onclick="showMainNot()"></i>
            <!-- notification reminder -->
            <p class="mainHeadNotificationIcon" onclick="showMainNot()">2</p>
            <!-- this is the real notification box-->
            <div 
                class="mainHeadNotificationContainer"
                id="mainHeadNotificationContainer"
                style="height: 0px; text-align: center;"
            >
                <p>Welcome to eKtalyst</p>
                <p>Nothing else to show</p>
                <i class="fas fa-times-circle" style="color: #fafafa;" onclick="showMainNot()"></i>                    
            </div>
            <!-- end of mainHeadNotificationBox -->
        </div>
        <!-- End of  mainHeadNotifiacction-->

        <div class="mainBanner vibeBgColor">
            <div class="container">
                <div class="row align-items-center justify-content-around">
                    <div class="col-sm-4 text-center">
                        <h2>Reach us.</h2>
                    </div>
                    <div class="col-sm-4 contactFeedButton">
                        <button id="viewContactButton" onclick="viewContactAndFeed('con')">CONTACT</button>
                        <button id="viewFeedButton" class="feedButton" onclick="viewContactAndFeed('feed')">FEEDBACK</button>
                    </div>
                </div>
                <div class="row align-items-start justify-content-around">
                    <div class="col-sm-4 text-center">
                        <img class="contactShowImage" src="./img/instructors/corporate-goals.png" alt="">
                    </div>
                    <div class="col-sm-4 contactAndFeed">
                        <div class="contactF" id="contact">
                            <form action="/contact" method="POST">
                                <label for="email">Email</label> <br>
                                <input type="email" name="email"> <br>
                                <label for="message">Message</label> <br>
                                <textarea name="message" id="" cols="20" rows="5"></textarea>
                                <button>SUBMIT</button>
                            </form>
                        </div>
                        <div class="feedback" id="feedback">
                            <form action="/contact" method="POST">
                                <label for="name">Name</label> <br>
                                <input type="name" name="name"> <br>
                                <label for="feedback">Feedback</label> <br>
                                <textarea name="feedback" id="" cols="20" rows="5"></textarea> <br>
                                <button>SUBMIT</button>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- footer -->
        <div class="myFooter">
            <div class="container">
                <div class="row justify-content-between">
                    <div class="col-sm-4">
                        <h6>Katalyst</h6>
                        <ul>
                            <li>
                                <a href="">Terms and Conditions</a>
                            </li>
                            <li>
                                <a href="">Privacy Policy</a>
                            </li>
                            <li>
                                <a href="">Refund Policy</a>
                            </li>
                        </ul>
                    </div>
                    <div class="col-sm-4">
                        <h6>Contact us on</h6>
                        <li>
                            Email: 
                            <a href="">contact@katalyst.com</a>
                        </li>
                        <li>
                            <div class="footerSocialContainer">
                                <div class="footerSocial">
                                    <i class="fab fa-instagram"></i>                                        
                                </div>
                                <div class="footerSocial">
                                    <i class="fab fa-twitter"></i>
                                </div>
                                <div class="footerSocial">
                                    <i class="fab fa-facebook"></i>                                        
                                </div>
                            </div>
                        </li>
                    </div>
                    <div class="col-sm-4 footerAppDownload">
                        <h6>Download Our App</h6>
                        <li>Play Store</li>
                        <li>App Store</li>
                    </div>
                </div>
            </div>
        </div>

        
            

    </div>
    <!-- end of body wrap -->

    <!-- Post load script -->
    <script src="./js/jquery-3.2.1.min.js"></script>
    <script src="./js/bootstrap.min.js"></script>
    <script src="./js/myAnimation.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/lax.js" ></script>
    <script src="./js/contactFeed.js"></script>
    <script src="./js/userDashboard.js"></script>


</body>
</html>