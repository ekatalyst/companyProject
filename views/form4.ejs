                                <option value="Lakshadweep">Lakshadweep</option>
                                <option value="Puducherry">Puducherry</option>
                        </select>
                    </div>
                </div>
                
                <br>
                <div class="wrap-input100 validate-input" data-validate = "Descriptive message is required">
					<span class="label-input200">Description about campaign</span>
					<input class="input100" type="text" name="input11" placeholder="Enter Here...">
					<span class="focus-input100"></span>
                </div>
                <div class="wrap-input100 validate-input" >
                    <span class="label-input200">Photo related to campaign</span>
					<input class="input100" type="file" name="userImageB" accept="image/png, image/jpeg, image/jpg">
					<span class="focus-input100"></span>
                </div>
                <div class="wrap-input100 validate-input" >
                    <span class="label-input200">Video related to campaign</span>
					<input class="input100" type="file" name="input13">
					<span class="focus-input100"></span>
                </div>
                

                <span class="contact101-form-title">
                        Donation Information
                    </span>

                    <div class="wrap-input100 input100-select">
                        <span class="label-input100">Type of crowdfunding</span>
                        <div>
                            <select class="selection-2" name="input14">
                                    <option value="Not Available">Please Select Option</option>
                                    <option value="Reward based crowdfunding">Reward based crowdfunding</option>
                                    <option value="Donation based crowdfunding">Donation based crowdfunding</option>
                                  
                            </select>
                        </div>
                        <span class="focus-input100"></span>
                    </div>

                    <div class="wrap-input101 validate-input" data-validate="valid range of number is required">
                        <div>
                        <span class="label-input200">Amount of required donation</span>
                        <input class="input100" type="number" name="input15" placeholder="1-100000000000Rs" min="1" max="100000000000">
                        </div>
                    </div>
                
                    <div class="wrap-input101 validate-input" data-validate="valid range of number is required">
                        <div>
                        <span class="label-input200">Time of campaign (mention the starting date and ending date)</span>
                        <input  type="date" name="input16[startDate]">
                        <br>
                        <input  type="date" name="input16[endDate]">
                        </div>
                    </div>
                
                    <div class="wrap-input100 validate-input" data-validate = "Valid name is required">
					<span class="label-input200">Describe the goal of the donation</span>
					<input class="input100" type="text" name="input17" placeholder="Enter Here...">
					<span class="focus-input100"></span>
<br>
               
<br>
                    <span class="contact101-form-title">
                            Reward Information
                        </span>

                        <div class="wrap-input100 validate-input" data-validate = "Descriptive Message is required">
                                <span class="label-input200">Describe your reward</span>
                                <textarea class="input100" name="input18" placeholder="Your message here..."></textarea>
                                <span class="focus-input100"></span>
                            </div>
                            <div class="wrap-input100 validate-input" data-validate = "Valid name is required">
                                <span class="label-input200">Describe the detail procedure of the giveaway of reward</span>
                                <input class="input100" type="text" name="input19" placeholder="Enter Here...">
                                <span class="focus-input100"></span>
                            </div>
                       
                            <div class="wrap-input100 validate-input" >
                                <span class="label-input200">Any photo/video related of the reward</span>
                                <input class="input100" type="file" name="userImageC" accept="image/png, image/jpeg, image/jpg">
                                <span class="focus-input100"></span>
                            </div>
                            
                               
    
                            <span class="contact101-form-title">
                                Contact Information
                            </span>


    <div class="wrap-input100 validate-input" data-validate = "Valid email is required: ex@abc.xyz">
            <span class="label-input200">Active Email ID of the founder of campaign</span>
            <input class="input100" type="email" name="input21" placeholder="Enter Here...">
            <span class="focus-input100"></span>
        </div>
        <div class="wrap-input100 validate-input" data-validate = "Valid phone number is required: ex@abc.xyz">
            <span class="label-input200">Active Phone Number of the founder of campaign</span>
            <input class="input100" type="number" name="input22" placeholder="Enter Here...">
            <span class="focus-input100"></span>
        </div>
        <div class="wrap-input100 validate-input" data-validate = "Valid link is required: ex@abc.xyz">
                <span class="label-input200">Social Media Links</span>
                <input class="input100" type="text" name="input23" placeholder="Enter Here...">
                <span class="focus-input100"></span>
        </div>
                
        <div class="wrap-input100 validate-input" data-validate = "Valid email is required: ex@abc.xyz">
            <span class="label-input200">Address of the founder of campaign</span>
            <input class="input100" type="text" name="input24" placeholder="Enter Here...">
            <span class="focus-input100"></span>
        </div>
        <div class="container-contact100-form-btn">
					<div class="wrap-contact100-form-btn">
						<div class="contact100-form-bgbtn"></div>
						<button class="contact100-form-btn">
							<span>
								Submit
								<i class="fa fa-long-arrow-right m-l-7" aria-hidden="true"></i>
							</span>
